{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Curso para treinair a criacao de um projeto totalmente",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "services:up": "docker compose -f infra/compose.yml up -d",
    "services:stop": "docker compose -f infra/compose.yml stop",
    "services:down": "docker compose -f infra/compose.yml down",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^15.5.9",
    "pg": "^8.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.7.4"
  }
}
